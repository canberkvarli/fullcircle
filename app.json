{
  "expo": {
    "name": "fullcircle",
    "slug": "fullcircle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fullcircle.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1",
      "deploymentTarget": "13.4",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow FullCircle to use your location to find nearby matches.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow FullCircle to use your location to find nearby matches.",
        "NSPhotoLibraryUsageDescription": "Allow Circle to access your photo library to share your photos.",
        "NSCameraUsageDescription": "Allow Circle to access your camera to take photos.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.fullcircle.app",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.856286042200-nv9vv4js8j3mqhu07acdbnf0hbp8feft"
            ]
          },
          {
            "CFBundleURLName": "com.fullcircle.app",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.856286042200-nv9vv4js8j3mqhu07acdbnf0hbp8feft"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.fullcircle.app",
      "gradleVersion": "8.8",
      "kotlinVersion": "1.9.25",
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true,
      "useLegacyPackaging": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCjHACigiDWxd6l8iZuwH6kyx5jbF3TKbg"
        }
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-firebase/app",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/auth",
      "expo-av",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FullCircle to use your location.",
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Circle to share your photos.",
          "cameraPermission": "Allow Circle to access your camera to take photos."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/poppins/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.4",
            "newArchEnabled": false,
            "flipper": false
          },
          "android": {
            "kotlinVersion": "1.9.25",
            "gradleVersion": "8.8"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "UNSPLASH_ACCESS_KEY": "dET0JZqiMDsodHvnQjp0uXIjMCcKfeq02l14rAWS4kw",
      "eas": {
        "projectId": "5e864641-186b-4d06-99f9-b7ad85439ff4"
      }
    }
  }
}

{
  "expo": {
    "name": "fullcircle",
    "slug": "fullcircle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fullcircle",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fullcircle.app",
      "buildNumber": "1",
      "deploymentTarget": "15.1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "fullcircle",
            "CFBundleURLSchemes": ["fullcircle"]
          }
        ]
      }
    },
    "android": {
      "package": "com.fullcircle.app",
      "gradleVersion": "8.8",
      "kotlinVersion": "1.9.25",
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "backgroundImage": "./assets/images/adaptive-icon.png"
      },
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true,
      "useLegacyPackaging": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCjHACigiDWxd6l8iZuwH6kyx5jbF3TKbg"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "fullcircle"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router", {
          "origin": "https://fullcircle.com"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-av",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FullCircle to use your location.",
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Circle to share your photos.",
          "cameraPermission": "Allow Circle to access your camera to take photos."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/poppins/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "kotlinVersion": "1.9.25",
            "gradleVersion": "8.8"
          }
        }
      ],
      [
        "@stripe/stripe-react-native", {
          "enableGooglePay": true,
          "publishableKey": "pk_test_51RlfjoGdeC038tAHh1GgI9jVQHxr7opaIPMByG4miVIO29K757hRG3DSkMeN6kNZEQjdTgGf7U891g5wwJhopfow00f1NQbc9s"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "UNSPLASH_ACCESS_KEY": "dET0JZqiMDsodHvnQjp0uXIjMCcKfeq02l14rAWS4kw",
      "eas": {
        "projectId": "5e864641-186b-4d06-99f9-b7ad85439ff4"
      }
    }
  }
}